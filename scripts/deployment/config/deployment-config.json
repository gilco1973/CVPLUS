{
  "deployment": {
    "batchSize": 6,
    "delayBetweenBatches": 25000,
    "maxConcurrentBatches": 1,
    "timeoutPerFunction": 120000,
    "retryAttempts": 3,
    "retryDelay": 10000
  },
  "validation": {
    "skipTypeCheck": false,
    "maxFileSizeBytes": 104857600,
    "maxFunctionCount": 300
  },
  "monitoring": {
    "enableHealthChecks": true,
    "enablePerformanceTracking": true,
    "logLevel": "info"
  }
}
