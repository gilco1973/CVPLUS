{
  "name": "@cvplus/e2e-flows",
  "version": "1.0.0",
  "description": "End-to-end testing flows for CVPlus - comprehensive user journey validation, submodule testing, and API validation",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "dev": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:contract": "jest tests/contract",
    "test:integration": "jest tests/integration",
    "test:unit": "jest tests/unit",
    "test:performance": "jest tests/performance",
    "test:load": "ts-node tests/load/run-load-tests.ts",
    "test:load:baseline": "ts-node tests/load/run-load-tests.ts --scenario=baseline",
    "test:load:medium": "ts-node tests/load/run-load-tests.ts --scenario=medium",
    "test:load:high": "ts-node tests/load/run-load-tests.ts --scenario=high",
    "test:load:stress": "ts-node tests/load/run-load-tests.ts --scenario=stress",
    "test:load:10k": "ts-node tests/load/run-load-tests.ts --scenario=stress --users=10000",
    "test:load:breakpoint": "ts-node tests/load/run-load-tests.ts --scenario=breakpoint",
    "test:load:recovery": "ts-node tests/load/run-load-tests.ts --scenario=recovery",
    "test:load:complete": "ts-node tests/load/run-load-tests.ts --scenario=complete",
    "test:load:jest": "jest tests/load/concurrent-load.test.ts",
    "test:load:validate": "ts-node tests/load/validate-load-tests.ts",
    "test:e2e": "npm run test:contract && npm run test:integration",
    "test:e2e:real-apis": "E2E_MOCK_APIS=false npm run test:e2e",
    "test:smoke": "jest tests/contract --testTimeout=30000",
    "test:ci": "jest --ci --coverage --maxWorkers=2",
    "lint": "eslint src tests --ext .ts,.tsx",
    "lint:fix": "eslint src tests --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
    "health-check": "node scripts/health-check.js",
    "mock-data:generate": "ts-node src/cli/mock-data-commands.ts generate",
    "mock-data:clean": "ts-node src/cli/mock-data-commands.ts clean",
    "mock-data:clear-cache": "rm -rf .cache/mock-data",
    "e2e:run": "ts-node src/cli/e2e-commands.ts run",
    "e2e:list": "ts-node src/cli/e2e-commands.ts list",
    "api-test:endpoint": "ts-node src/cli/api-test-commands.ts endpoint",
    "api-test:suite": "ts-node src/cli/api-test-commands.ts suite",
    "scenario:complete-journey": "ts-node src/scenarios/complete-journey.ts",
    "scenario:submodule-integration": "ts-node src/scenarios/submodule-integration.ts",
    "scenario:api-contracts": "ts-node src/scenarios/api-contracts.ts",
    "scenario:load-test": "ts-node src/scenarios/load-test.ts",
    "scenario:regression-check": "ts-node src/scenarios/regression-check.ts",
    "dev:start": "concurrently \"npm run build:watch\" \"npm run test:watch\"",
    "test:report": "jest --coverage --coverageReporters=html",
    "firebase:emulators": "firebase emulators:start --only firestore,auth,storage",
    "validate:quickstart": "ts-node scripts/validate-quickstart.ts",
    "type-check": "tsc --noEmit",
    "clean": "rimraf dist"
  },
  "keywords": [
    "e2e-testing",
    "cvplus",
    "firebase",
    "jest",
    "typescript",
    "test-automation",
    "api-testing",
    "mock-data"
  ],
  "author": "CVPlus Development Team",
  "license": "MIT",
  "dependencies": {
    "@faker-js/faker": "^8.4.1",
    "axios": "^1.6.7",
    "commander": "^12.0.0",
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^4.6.0",
    "joi": "^17.12.1",
    "lodash": "^4.17.21",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.11.19",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "firebase-tools": "^13.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "@cvplus/core": "file:../core"
  },
  "files": [
    "dist",
    "src",
    "tests",
    "firebase.json",
    ".env.example"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:gilco1973/cvplus-e2e-flows.git"
  },
  "bugs": {
    "url": "https://github.com/gilco1973/cvplus-e2e-flows/issues"
  },
  "homepage": "https://github.com/gilco1973/cvplus-e2e-flows#readme",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  }
}
