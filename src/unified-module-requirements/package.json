{
  "name": "@cvplus/unified-module-requirements",
  "version": "1.0.0",
  "description": "CVPlus Unified Module Requirements validation and architectural compliance system",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.build.json && chmod +x dist/bin/*.js",
    "build:watch": "tsc --watch",
    "start": "node dist/bin/umr-server.js",
    "dev": "ts-node src/bin/umr-server.ts",
    "cli": "ts-node src/bin/umr-cli.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepack": "npm run clean && npm run build"
  },
  "bin": {
    "umr": "./dist/bin/umr-cli.js",
    "umr-server": "./dist/bin/umr-server.js"
  },
  "keywords": [
    "cvplus",
    "module",
    "validation",
    "architecture",
    "requirements",
    "compliance",
    "typescript",
    "monorepo"
  ],
  "author": "CVPlus Team",
  "license": "MIT",
  "dependencies": {
    "commander": "^11.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.2.1",
    "glob": "^10.3.10",
    "helmet": "^7.1.0",
    "http-proxy-middleware": "^3.0.5",
    "semaphore": "^1.1.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.10.4",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cvplus/unified-module-requirements.git"
  },
  "bugs": {
    "url": "https://github.com/cvplus/unified-module-requirements/issues"
  },
  "homepage": "https://github.com/cvplus/unified-module-requirements#readme",
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ]
}
